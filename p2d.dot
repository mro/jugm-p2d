#!/usr/bin/dot -Tpdf -o p2d.pdf
/*
        See http://www.graphviz.org/Documentation.php
*/
digraph "Piccolo2d Ueberblick" {
	rankdir=LR;
	graph [bgcolor=transparent]
	label="Piccolo2D Stammbaum";
	pi2 [label="piccolo2d", URL="http://piccolo2d.org/", fillcolor=lightblue2, style=filled];
	p2d [label="piccolo", URL="http://www.cs.umd.edu/hcil/jazz/"];
	".net" [label=".NET", URL="http://de.wikipedia.org/wiki/.NET"];
	"c++" [label="C++", URL="http://de.wikipedia.org/wiki/C%2B%2B"];
	"local tools" [URL="http://doi.acm.org/10.1145/237091.237116"];
	"pad++" [label="PAD++", URL="http://www.cs.umd.edu/hcil/pad++/"];
	chevron [label=Chevron, URL="http://www.chevron.com/"];
	darpa [label=DARPA, URL="http://www.nsf.gov/"];
	gdi [label="GDI+", URL="http://de.wikipedia.org/wiki/GDI%2B"];
	ge [label="Google Earth", URL="http://de.wikipedia.org/wiki/Google_Earth"];
	grap [label="Graph", URL="http://de.wikipedia.org/wiki/Graph_(Graphentheorie)"];
	java [label=Java, URL="http://de.wikipedia.org/wiki/Java_(Technik)"];
	java2d [label=Java2D, URL="http://en.wikipedia.org/wiki/Java_2D"];
	java3d [label=Java3D, URL="http://de.wikipedia.org/wiki/Java_3D"];
	javafx [label=JavaFX, URL="http://de.wikipedia.org/wiki/JavaFX"];
	jazz [URL="http://piccolo2d.org/learn/about.html"];
	kidpad [label="KidPad", URL="http://www.cs.umd.edu/hcil/kiddesign/kidpad.shtml"];
	ms [label="Microsoft Research", URL="http://research.microsoft.com/"];
	nsf [label=NSF, URL="http://www.nsf.gov/"];
	ogre [label=OGRE, URL="http://de.wikipedia.org/wiki/OGRE"];
	opengl [label=OpenGL, URL="http://de.wikipedia.org/wiki/Opengl"];
#	openvg [label=OpenVG, URL="http://de.wikipedia.org/wiki/OpenVG"];
	osg [label=OpenSceneGraph, URL="http://en.wikipedia.org/wiki/OpenSceneGraph"];
	posy [label=PhotoSynth, URL="http://www.ted.com/index.php/talks/blaise_aguera_y_arcas_demos_photosynth.html"];
	scenegraph [URL="https://scenegraph.dev.java.net/"];
	seadragon [label=SeaDragon, URL="http://en.wikipedia.org/wiki/DeepZoom"];
	sg [label=Szenengraph, URL="http://de.wikipedia.org/wiki/Szenengraph"];
	svg [label=SVG, URL="http://de.wikipedia.org/wiki/Scalable_Vector_Graphics"];
	umd [label="University of Maryland", URL="http://www.cs.umd.edu/hcil/"];
	zui [label=ZUI, URL="http://de.wikipedia.org/wiki/ZUI"];
	safa [label="Mobile Safari", URL="http://www.apple.com/iphone/features/safari.html"];

pi2 -> p2d;
"pad++" -> "c++";
"pad++" -> umd;
"pad++" -> zui;
gdi -> ".net";
ge -> zui;
java2d -> java;
java2d -> opengl;
java3d -> java;
java3d -> opengl;
java3d -> sg;
javafx -> scenegraph;
jazz -> "pad++";
